{
  "name": "frappe-bench-console-playground",
  "displayName": "Frappe Bench Console Playground",
  "description": "Play around with Frappe functions inside Bench Console",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frappe-bench-console-playground.run-func-in-bench-console",
        "title": "Bench Console: Run Function in Bench Console"
      },
      {
        "command": "frappe-bench-console-playground.import-all-in-bench-console",
        "title": "Bench Console: Import All in Bench Console"
      },
      {
        "command": "frappe-bench-console-playground.import-in-bench-console",
        "title": "Bench Console: Import in Bench Console"
      }
    ],
    "submenus": [
      {
        "id": "console-playground",
        "label": "Console Playground"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "console-playground",
          "when": "editorLangId == python",
          "group": "1_modification"
        }
      ],
      "console-playground": [
        {
          "command": "frappe-bench-console-playground.run-func-in-bench-console",
          "group": "playground@1"
        },
        {
          "command": "frappe-bench-console-playground.import-all-in-bench-console",
          "group": "playground@2"
        },
        {
          "command": "frappe-bench-console-playground.import-in-bench-console",
          "group": "playground@3"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:python"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.13",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  }
}
