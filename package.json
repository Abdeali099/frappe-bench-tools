{
  "name": "frappe-function-tester",
  "displayName": "Frappe Function Tester",
  "description": "Run function in bench console",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frappeTester.runFunction",
        "title": "Run Function in Bench Console"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "frappeTester.runFunction",
          "when": "editorLangId == python",
          "group": "navigation"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:python"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
