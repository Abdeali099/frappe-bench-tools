{
  "name": "frappe-bench-tools",
  "publisher": "abdeali",
  "displayName": "Frappe Bench Tools",
  "description": "Run Frappe Bench commands directly from VS Code â€” open console, import modules, and execute functions with ease.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Programming Languages"
  ],
  "icon": "/assets/images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Abdeali099/frappe-bench-tools"
  },
  "homepage": "https://github.com/Abdeali099/frappe-bench-tools",
  "bugs": {
    "url": "https://github.com/Abdeali099/frappe-bench-tools/issues"
  },
  "license": "MIT",
  "keywords": [
    "frappe",
    "erpnext",
    "bench",
    "console",
    "developer-tools",
    "python",
    "automation"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onLanguage:python"
  ],
  "contributes": {
    "commands": [
      {
        "command": "frappe-bench-tools.open-bench-console",
        "title": "Open Bench Console"
      },
      {
        "command": "frappe-bench-tools.paste-to-bench-console",
        "title": "Paste to Bench Console"
      },
      {
        "command": "frappe-bench-tools.paste-clipboard-to-bench-console",
        "title": "Paste Clipboard to Bench Console"
      },
      {
        "command": "frappe-bench-tools.import-in-bench-console",
        "title": "Import in Bench Console"
      },
      {
        "command": "frappe-bench-tools.import-all-in-bench-console",
        "title": "Import All in Bench Console"
      },
      {
        "command": "frappe-bench-tools.import-as-in-bench-console",
        "title": "Import As in Bench Console"
      },
      {
        "command": "frappe-bench-tools.run-func-in-bench-console",
        "title": "Run Function in Bench Console"
      },
      {
        "command": "frappe-bench-tools.bench-execute-command",
        "title": "Bench Execute Python Function"
      }
    ],
    "submenus": [
      {
        "id": "frappe-bench",
        "label": "Frappe Bench"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "frappe-bench",
          "when": "editorLangId == python",
          "group": "6_copypath"
        }
      ],
      "frappe-bench": [
        {
          "command": "frappe-bench-tools.open-bench-console",
          "group": "bench1@1"
        },
        {
          "command": "frappe-bench-tools.paste-to-bench-console",
          "group": "bench2@1"
        },
        {
          "command": "frappe-bench-tools.paste-clipboard-to-bench-console",
          "group": "bench2@2"
        },
        {
          "command": "frappe-bench-tools.import-in-bench-console",
          "group": "bench3@1"
        },
        {
          "command": "frappe-bench-tools.import-all-in-bench-console",
          "group": "bench3@2"
        },
        {
          "command": "frappe-bench-tools.import-as-in-bench-console",
          "group": "bench3@3"
        },
        {
          "command": "frappe-bench-tools.run-func-in-bench-console",
          "group": "bench4@1"
        },
        {
          "command": "frappe-bench-tools.bench-execute-command",
          "group": "execute@1"
        }
      ]
    },
    "configuration": [
      {
        "title": "Frappe Bench Tools: General",
        "properties": {
          "frappeBenchTools.siteName": {
            "type": "string",
            "default": "frappe.localhost",
            "description": "The site name to use when running bench commands (otherwise default bench site will be used)."
          }
        }
      },
      {
        "title": "Frappe Bench Tools: Console",
        "properties": {
          "frappeBenchTools.consoleTerminalName": {
            "type": "string",
            "default": "Bench Console",
            "description": "The name of the bench console terminal."
          },
          "frappeBenchTools.autoReload": {
            "type": "boolean",
            "default": true,
            "description": "Automatically reload the bench console when files change."
          }
        }
      },
      {
        "title": "Frappe Bench Tools: Execute",
        "properties": {
          "frappeBenchTools.executeTerminalName": {
            "type": "string",
            "default": "Bench Execute",
            "description": "The name of the bench execute terminal."
          },
          "frappeBenchTools.acceptArgsForExecute": {
            "type": "boolean",
            "default": true,
            "description": "Whether to accept arguments for the execute command."
          },
          "frappeBenchTools.acceptKwargsForExecute": {
            "type": "boolean",
            "default": true,
            "description": "Whether to accept keyword arguments for the execute command."
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "release": "standard-version && git push --follow-tags && vsce publish",
    "package": "npx vsce package"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.13",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.2",
    "eslint": "^9.13.0",
    "standard-version": "^9.5.0"
  }
}
